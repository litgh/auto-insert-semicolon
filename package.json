{
    "name": "auto-insert-semicolon",
    "displayName": "Auto Insert Semicolon",
    "description": "Append semicolon to the end of current line.",
    "version": "0.0.1",
    "publisher": "lizheng",
    "engines": {
        "vscode": "^1.13.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.auto-insert-semicolon"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.auto-insert-semicolon",
                "title": "Auto Insert Semicolon"
            }
        ],
        "keybindings": [
            {
                "key": "alt+enter",
                "when": "editorTextFocus",
                "command": "extension.auto-insert-semicolon"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}